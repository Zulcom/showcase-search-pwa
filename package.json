    "framer-motion": "^11.15.0",
    "p-retry": "^6.2.1",
    "react-hotkeys-hook": "^4.6.1",
    "use-debounce": "^10.0.4",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.47.1",
    "@playwright/test": "^1.49.1",
    "@testing-library/dom": "^10.4.0",
